{
  "comments": {
    "lineComment": "//",
    "blockComment": ["/*", "/**", "*/"]
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    { "open": "/**", "close": " */", "notIn": ["string"] },
    { "open": "/*", "close": " */", "notIn": ["string"] }
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""]
  ],
  "wordPattern": "[A-Za-z0-9_]{1,})",
  "indentationRules": {
    "increaseIndentPattern": "{",
    "decreaseIndentPattern": "}"
  }
}
